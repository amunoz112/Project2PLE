x,y,z  
add = function (a, b) do
    a + b
end add

max = function (a, b) do
    if a > b then
        a
    else
        b
    end
end max

sum = function (n) do
    result = from 1 to n
    total
    for i result do
        total + i
    end
end sum

sign = function (n) do
    cond n do
        n > 0 -> 1
        n < 0 -> -1
        n = 0 -> 0
    end
end sign

point = data with x, y
    point = struct (x, y)
end point

calculate = function (a, b) do
    result
    result = a ** 2 + b * 3 - 5
    result
end calculate

isvalid = function (x) do
    x > 0 and x < 100
end isvalid